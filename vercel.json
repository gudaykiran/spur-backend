{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/health",
            "dest": "/api/health.ts"
        },
        {
            "src": "/test",
            "dest": "/api/test.ts"
        },
        {
            "src": "/api/chat",
            "dest": "/api/chat.ts",
            "methods": [
                "POST",
                "OPTIONS"
            ]
        },
        {
            "src": "/api/migrate",
            "dest": "/api/migrate.ts",
            "methods": [
                "POST"
            ]
        }
    ]
}